{
  "name": "vue-pure-frame",
  "version": "1.20240313.3",
  "scripts": {
    "serve": "cross-env CURRENT_ENV=dev vite",
    "serve:uat": "cross-env CURRENT_ENV=uat vite --mode uat",
    "build": "run-p type-check && cross-env CURRENT_ENV=prod run-p build-only",
    "preview": "cross-env CURRENT_ENV=prod vite preview",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "syncapi": "tsx scripts/syncApi.ts"
  },
  "types": [
    "vite/client"
  ],
  "devDependencies": {
    "@playwright/test": "^1.36.1",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "sass": "^1.64.2",
    "sharp": "^0.32.4",
    "svgo": "^3.0.2",
    "swagger-typescript-api": "^13.0.3",
    "tsx": "^4.1.1",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-compression2": "^0.10.3",
    "vite-plugin-image-optimizer": "^1.1.6",
    "vite-plugin-javascript-obfuscator": "^3.1.0",
    "vite-plugin-update-version": "^1.0.8",
    "vizion": "^2.2.1",
    "vue-tsc": "^1.8.6"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^2.4.2",
    "less": "^4.1.1",
    "sass": "^1.32.8",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  }
}
